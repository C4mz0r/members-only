<h1>Log In</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br> 
  <%= f.submit "Log In" %>
   
<% end%>

<%= link_to 'Or see what dirt has been posted about you by our members!', posts_path %>
